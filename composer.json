{
    "name": "spryker-eco/shop-ui-compatibility",
    "description": "ShopUi Compatibility module",
    "license": "proprietary",
    "require": {
        "php": ">=7.1",
        "spryker/kernel": "^3.0.0",
        "spryker/twig": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/config": "*",
        "spryker/silex": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\": "src/SprykerEco/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerEcoTest\\": "tests/SprykerEco/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
